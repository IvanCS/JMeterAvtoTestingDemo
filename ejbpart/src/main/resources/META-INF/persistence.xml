<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
   <persistence-unit name="persistence_unit" >


       <class>com.tsystems.jmeteravtotestingdemo.backend.ejbs.entities.DepartmentEntity</class>
       <class>com.tsystems.jmeteravtotestingdemo.backend.ejbs.entities.EmployeeEntity</class>
       <class>com.tsystems.jmeteravtotestingdemo.backend.ejbs.entities.SysUserEntity</class>
       <class>com.tsystems.jmeteravtotestingdemo.backend.ejbs.entities.RoleEntity</class>
       <properties>
           <property name="toplink.jdbc.url" value="jdbc:hsqldb:hsql://localhost/employeesDB"/>
           <property name="toplink.jdbc.driver" value="org.hsqldb.jdbc.JDBCDriver"/>
           <property name="toplink.jdbc.user" value="sa"/>
           <property name="toplink.jdbc.password" value=""/>
           <property name="hibernate.connection.url" value="jdbc:hsqldb:hsql://localhost/employeesDB"/>
           <property name="hibernate.connection.driver_class" value="org.hsqldb.jdbc.JDBCDriver"/>
           <property name="hibernate.connection.username" value="sa"/>
           <property name="hibernate.connection.password" value=""/>
           <property name="openjpa.ConnectionURL" value="jdbc:hsqldb:hsql://localhost/employeesDB"/>
           <property name="openjpa.ConnectionDriverName" value="org.hsqldb.jdbc.JDBCDriver"/>
           <property name="openjpa.ConnectionUserName" value="sa"/>
           <property name="openjpa.ConnectionPassword" value=""/>
           <property name="eclipselink.jdbc.url" value="jdbc:hsqldb:hsql://localhost/employeesDB"/>
           <property name="eclipselink.jdbc.driver" value="org.hsqldb.jdbc.JDBCDriver"/>
           <property name="eclipselink.jdbc.user" value="sa"/>
           <property name="eclipselink.jdbc.password" value=""/>
       </properties>
   </persistence-unit>
</persistence>